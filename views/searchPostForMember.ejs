<%- include memberheader %>
<div style="padding:30px">
</div>

<div class="container">
    <div class="page-header text-center">
        <h1><span class="fa fa-anchor"></span> Search Posts</h1>
    </div>
<label> Search Attribute </label>
<form class="form-inline" role="form" method="post" action="/searchPostForMember">
  <select class="form-control" id="searchparam" name="searchparam">
                   
                    <option value="poster">poster</option>
                    <option value="title">title</option>
                    <option value="createDate">Create Date</option>
                    <option value="actionType">Action Type</option>
                    <option value="categoryFlag">Category Flag</option>
                    <option value="categoryType">Category Type</option>                    
  </select> &nbsp&nbsp&nbsp&nbsp
  <div class="form-group">
    <label class="sr-only">Search String</label>
    <input type="text" class="form-control" placeholder="Enter Search String" id="str" name="str">
  </div>
  &nbsp&nbsp&nbsp&nbsp
  <button type="submit" class="btn btn-primary">Search<i class="icon-search"></i></button>
</form>
<br>

<% posts.forEach( function ( posts ){ %>

    <div class="panel panel-default">
        <div class="panel-heading">
            <a href="/post-view-only/<%= posts.id %>"><h3 class="panel-title"> <%= posts.title %></h3> </a>
            
        </div>        
	</div>
<% }); %>